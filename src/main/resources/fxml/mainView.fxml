<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.lang.String?>
<?import java.net.URL?>
<GridPane alignment="CENTER" hgap="20.0" prefHeight="450" prefWidth="700" styleClass="root" vgap="20.0" xmlns="http://javafx.com/javafx/8.0.112"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.jiayu.workhome.WorkhomeController">

    <Button fx:id="pausePrintBtn"  onAction="#pausePrint" disable="true"  text="暂停" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Button fx:id="startPrintBtn"  onAction="#startPrint"  text="开始" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <Text fx:id="tipMsgText"  GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1"/>

    <stylesheets>
        <URL value="@../css/main.css"/>
    </stylesheets>



    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>

    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>

</GridPane>
